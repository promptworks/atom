"*":
  core:
    themes: [
      "dracula-ui"
      "dracula-theme"
    ]

  editor:
    showIndentGuide: true

  "markdown-preview":
    useGitHubStyle: true

  react:
    enabledForAllJavascriptFiles: true

  "vim-mode-plus":
    incrementalSearch: true

  welcome:
    showOnStartup: false

  zentabs:
    maximumOpenedTabs: 8
